QGroupBox#grayBar {
    border: 0 none;
    outline:0;
    border-radius: 0;
    background-color: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0,
              stop: 0 #838383, stop: .25 #979797, stop: .5 #a7a7a7, stop: .75 #b4b4b4,stop: 1 #cacaca);
}
QGroupBox#header {
    background-color: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0,
          stop: 0 #802727, stop: .25 #a03635, stop: .5 #b13e3e, stop: .75 #b74040, stop: 1 #bf4343);
}
QPushButton#upButton, QPushButton#dnButton, QPushButton#pixmap  {
	outline:0;
	border: 0 none;
	background-color:  transparent;
}

