QCheckBox {
    border: 2px solid #ccc;
    color: white;
    outline:0;
    padding: 3px 6px 5px 6px;
    border-radius: 10px;
    background-color: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0,
              stop: 0 #802727, stop: .25 #a03635, stop: .5 #b13e3e, stop: .75 #b74040, stop: 1 #bf4343);
}
QCheckBox::indicator:unchecked {
	image: url(:images/checkbox_off2.png);
}
QCheckBox::indicator:checked {
	image: url(:images/checkbox_on2.png);
}
QPushButton, QPushButton#cool, QPushButton#heat, QPushButton#red, QPushButton#off  {
    border: 2px solid #ccc;
    color: white;
    outline:0;
    padding: 2px 6px 6px 6px;
    border-radius: 10px;
    background-color: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0,
              stop: 0 #802727, stop: .25 #a03635, stop: .5 #b13e3e, stop: .75 #b74040, stop: 1 #bf4343);
}
QPushButton:pressed {
    border: 2px solid firebrick;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
              stop: 0 #c294b7, stop: .25 #d0a4c8, stop: .5 #d9afd2, stop: .75 #d9b8d7,stop: 1 #d8c4df);
}
QPushButton:disabled, QPushButton#heat:disabled, QPushButton#cool:disabled  {
    border: 2px solid #ccc;
    background-color: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0,
              stop: 0 #d8c4df, stop: .25 #a3a3a3, stop: .5 #b2b2b2, stop: .75 #cbcbcb,stop: 1 #dfdfdf);
}
QPushButton#keyboard {
    color: white;
    border: 2px solid #ccc;
    padding: 3px 6px 5px 6px;
    border-radius: 10px;
    outline:0;
    background-color: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0,
              stop: 0 #d8c4df, stop: .25 #a3a3a3, stop: .5 #b2b2b2, stop: .75 #cbcbcb,stop: 1 #dfdfdf);
}
QPushButton#keyboard:pressed {
